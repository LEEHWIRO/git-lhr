<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="member">
		
		<select id="selectMemberById" parameterClass="String" resultClass="kr.or.ddit.member.vo.MemberVO">
		
		select * from member 
		where 1=1
		and mem_id = #memId#
		</select>

		
		
</sqlMap>			





